{
  "name": "tslib-esbuild",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "ts-types": " tsc --emitDeclarationOnly --outDir lib",
    "build": "rimraf lib && node ./esbuild.js && npm run ts-types",
    "test": "ts-mocha -p tsconfig.test.json test/**/*.spec.ts",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --ext .ts --fix src",
    "docs": "typedoc ./src --out ./docs && touch ./docs/.nojekyll",
    "precommit": "npm build",
    "prepush": "npm test",
    "prepare": "husky install"
  },
  "author": "Joaqim Planstedt",
  "license": "MIT",
  "devDependencies": {
    "@programic/eslint-config-typescript": "^7.0.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.3.6",
    "esbuild": "^0.14.31",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "html-minifier-terser": "^7.0.0-alpha.2",
    "husky": "^7.0.4",
    "mocha": "^9.2.2",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-mocha": "^9.0.2",
    "typedoc": "^0.22.13",
    "typescript": "^4.6.3"
  }
}
